<h1>Cross Stitch</h1>
<h2>Newest Patterns</h2>
<div id="patterns">
  <!-- Generate Pattern Divs from Results -->
  {{#each etsyData.results}}
  <a href="{{this.url}}" target="_blank">
    <div class="pattern">
      <p>{{this.title}}</p>
      <img src={{this.MainImage.url_170x135}} alt={{this.title}}>
    </div>
  </a>
  {{/each}}
</div>
